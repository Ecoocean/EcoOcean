{
  "name": "functions",
  "scripts": {
    "start": "env-cmd -f .env.production node src/index.js",
    "startContainersLocal": "docker-compose up -d",
    "stopContainersLocal": "docker-compose down --volumes --remove-orphans --rmi local",
    "gcp-build": "tsc -p .",
    "migrate": "node-pg-migrate",
    "migrateLocal": "env-cmd -f .env.development node-pg-migrate --no-verbose",
    "start:dev": "env-cmd -f .env.development ts-node-dev --respawn src/index.ts",
    "start:test": "env-cmd -f .env.test ts-node-dev --respawn src/index.ts",
    "migrateUpLocal": "npm run migrateLocal up",
    "migrateUp": "npm run migrate up",
    "test": "jest --passWithNoTests",
    "build": "tsc -p ."
  },
  "engines": {
    "node": "16"
  },
  "main": "./src/index.js",
  "dependencies": {
    "@google-cloud/firestore": "4.15.1",
    "@graphile-contrib/pg-simplify-inflector": "6.1.0",
    "@graphile/pg-pubsub": "4.12.2",
    "@graphile/postgis": "0.1.0",
    "@graphql-tools/schema": "8.3.5",
    "@types/firebase": "3.2.1",
    "apollo-datasource-firestore": "3.0.0",
    "apollo-log": "1.1.0",
    "apollo-server": "3.6.6",
    "apollo-server-express": "3.6.6",
    "env-cmd": "10.1.0",
    "express-winston": "4.2.0",
    "fastify": "3.27.2",
    "fastify-express": "0.3.3",
    "firebase": "9.3.0",
    "firebase-admin": "10.0.2",
    "firebase-functions": "3.19.0",
    "geofire-common": "5.2.0",
    "graphql": "15.7.2",
    "graphql-subscriptions": "1.2.1",
    "graphql-upload": "12.0.0",
    "jest": "27.5.1",
    "middie": "6.0.0",
    "node-fetch": "3.0.0",
    "node-geocoder": "3.29.0",
    "node-open-geocoder": "4.0.1",
    "node-pg-migrate": "6.2.1",
    "pg": "8.7.3",
